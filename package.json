{
  "name": "image-web",
  "version": "0.0.1",
  "description": "Image optimisation and compression for the web.",
  "bin": "./app/dist/esm/cli.js",
  "main": "./app/dist/esm/imageWeb.js",
  "types": "./app/dist/esm/imageWeb.d.ts",
  "exports": {
    "node": {
      "import": "./app/dist/esm/imageWeb.js",
      "require": "./app/dist/cjs/imageWeb.js"
    },
    "default": "./app/dist/esm/imageWeb.js"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.prod.cjs.json && tsc -p ./tsconfig.prod.esm.json",
    "dev": "npm run devNode",
    "devWeb": "rollup --config rollup.web.dev.config.js -w",
    "devNode": "rollup --config rollup.node.dev.config.js -w",
    "deploy": "npm run build && npm publish",
    "repl": "del-cli ./test_out/** && node ./repl/dist/imageWeb-repl.js",
    "start": "npm run repl",
    "buildTest": "rollup --config rollup.node.test.config.js",
    "test": "npm run buildTest && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianMairinger/imageWeb.git"
  },
  "keywords": [
    "web",
    "image",
    "compression",
    "optimisation",
    "min",
    "minification",
    "img",
    "picture"
  ],
  "author": "Maximilian Mairinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianMairinger/imageWeb/issues"
  },
  "homepage": "https://github.com/maximilianMairinger/imageWeb#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/cli-progress": "^3.8.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.4",
    "@types/sharp": "^0.26.1",
    "del-cli": "^3.0.1",
    "jest": "^26.6.3",
    "rollup": "^2.18.1",
    "tslib": "2.0.0",
    "typescript": "^4.0.2",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "cli-progress": "^3.8.2",
    "commander": "^6.2.1",
    "is-image": "^3.0.0",
    "log-update": "^4.0.0",
    "sharp": "^0.27.0",
    "slash": "^3.0.0",
    "xrray": "^4.7.3"
  }
}
